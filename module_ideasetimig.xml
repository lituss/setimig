<?xml version="1.0" encoding="UTF-8"?>
<project name="module_ideasetimig" default="compile.module.ideasetimig">
  <dirname property="module.ideasetimig.basedir" file="${ant.file.module_ideasetimig}"/>
  
  <property name="module.jdk.home.ideasetimig" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.ideasetimig" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.ideasetimig" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.ideasetimig" value="-encoding UTF-8 -source 1.7 -target 1.7 ${compiler.args}"/>
  
  <property name="ideasetimig.output.dir" value="${module.ideasetimig.basedir}/out/production/ideaSetimig"/>
  <property name="ideasetimig.testoutput.dir" value="${module.ideasetimig.basedir}/out/test/ideaSetimig"/>
  
  <path id="ideasetimig.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="ideasetimig.module.production.classpath">
    <path refid="${module.jdk.classpath.ideasetimig}"/>
  </path>
  
  <path id="ideasetimig.runtime.production.module.classpath">
    <pathelement location="${ideasetimig.output.dir}"/>
  </path>
  
  <path id="ideasetimig.module.classpath">
    <path refid="${module.jdk.classpath.ideasetimig}"/>
    <pathelement location="${ideasetimig.output.dir}"/>
  </path>
  
  <path id="ideasetimig.runtime.module.classpath">
    <pathelement location="${ideasetimig.testoutput.dir}"/>
    <pathelement location="${ideasetimig.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.ideasetimig">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.ideasetimig">
    <patternset refid="excluded.from.module.ideasetimig"/>
  </patternset>
  
  
  <target name="compile.module.ideasetimig" depends="compile.module.ideasetimig.production,compile.module.ideasetimig.tests" description="Compile module ideaSetimig"/>
  
  <target name="compile.module.ideasetimig.production" depends="register.custom.compilers" description="Compile module ideaSetimig; production classes"/>
  
  <target name="compile.module.ideasetimig.tests" depends="register.custom.compilers,compile.module.ideasetimig.production" description="compile module ideaSetimig; test classes" unless="skip.tests"/>
  
  <target name="clean.module.ideasetimig" description="cleanup module">
    <delete dir="${ideasetimig.output.dir}"/>
    <delete dir="${ideasetimig.testoutput.dir}"/>
  </target>
</project>